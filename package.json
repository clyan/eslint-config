{
  "name": "@clyan/eslint-config-monorepos",
  "version": "0.0.1",
  "description": "Clyan's ESLint config presets",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "prepare": "husky install",
    "release:dry": "release-it --dry-run --no-npm",
    "release:info": "release-it --release-version",
    "release:beta:no-npm": "release-it --preRelease=beta --no-npm",
    "release:beta": "release-it --preRelease=beta",
    "release:no-npm": "release-it --no-npm",
    "release:no-increment": "release-it --no-increment --no-npm",
    "release": "release-it"
  },
  "keywords": [],
  "packageManager": "pnpm@7.3.0",
  "author": "Clyan <2429335889@qq.com>(https://github.com/clyan/)",
  "bugs": {
    "url": "https://github.com/clyan/eslint-config/issues"
  },
  "homepage": "https://github.com/clyan/eslint-config#readme",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@release-it/conventional-changelog": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^10.2.7",
    "release-it": "^15.9.3",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --cache --fix"
    ]
  },
  "contributors": [
    "Clyan (https://github.com/clyan)"
  ]
}
